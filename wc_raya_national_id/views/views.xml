<odoo>
  <data>
  <record model="ir.ui.view" id="inherit_national_id_emp_filter">
  		<field name="name">inherit_prject_emp_filter</field>
  		<field name="model">hr.employee</field>
  		<field name="inherit_id" ref="hr.view_employee_filter"/>
  		<field name="arch" type="xml">
  			<field name="name" position="after">
          <!-- <field name="hr_id"/> -->
          <field name="identification_id"/>
        </field>
  		</field>
  	</record>
<record id="hr_applicant_form_national_id_view_inherit" model="ir.ui.view">
        <field name="name">hr.applicant.form.hrr.inherit</field>
        <field name="model">hr.applicant</field>
        <field name="inherit_id" ref="hr_recruitment.hr_applicant_view_form"/>
        <field name="arch" type="xml">
            <field name="partner_name" position="after">
                <div class="oe_title">
                    <label for="national_id" class="oe_edit_only"/>
                    <h3><field name="national_id"/></h3>
                </div>
            </field>
 </field>
    </record>
  </data>
</odoo>